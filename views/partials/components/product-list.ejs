<template id="product_list">


    <section class="product-list-content" data-event="click">
    
        <div class="card hide" data-for="item of products" >
            <div class="card-body">
                <div class="product" data-product-id="{{item.id}}">
                    <div class="product-image-container">
                        <img data-src="{{item.image}}" alt="{{item.category}}">
                        <div class="quickview">
                            <a class="btn btn-warning" type="button" data-role="quickview">Quick View</a>
                        </div>
                    </div>
                    <div class="product-text-container">
                        <p>{{item.category}}</p>
                        <p>{{item.title}}</p>
                        <!-- <p>{{item.description}}</p> -->
                        <!-- <p>{{item.id}}</p> -->
                        <p>PHP {{item.price}}</p>
    
                    </div>

                </div>
            </div>
        </div>
    </section>
    

</template>